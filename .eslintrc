{
  "extends": "airbnb",
  "rules": {
    "arrow-parens": ["error", "always"],
    "function-paren-newline": ["error", "consistent"],
    "import/no-extraneous-dependencies": 0,
    "max-len": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/require-default-props": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  }
}
